<?xml version="1.0"?>
<Container version="2">
  <Name>duplicati</Name>
  <Repository>lscr.io/linuxserver/duplicati:latest</Repository>
  <Registry>https://lscr.io/linuxserver/duplicati/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/57181-support-linuxserverio-duplicati/</Support>
  <Project>https://www.duplicati.com/</Project>
  <Overview>Duplicati is a backup client that securely stores encrypted, incremental, compressed backups on cloud storage services and remote file servers.</Overview>
  <Category>Backup:</Category>
  <WebUI>http://[IP]:[PORT:8200]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/duplicati.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/duplicati.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1640995200</DateInstalled>
  <DonateText>Donations</DonateText>
  <DonateLink>https://www.linuxserver.io/donate</DonateLink>
  <Requires/>
  <Config Name="WebUI" Target="8200" Default="8200" Mode="tcp" Description="WebUI port" Type="Port" Display="always" Required="true" Mask="false"/>
  <Config Name="Appdata" Target="/config" Default="/mnt/user/appdata/duplicati" Mode="rw" Description="Contains all relevant configuration files." Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Backups" Target="/backups" Default="/mnt/user/backups" Mode="rw" Description="Path to backup location" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Source" Target="/source" Default="/mnt/user/source" Mode="ro" Description="Path to source files to backup" Type="Path" Display="always" Required="false" Mask="false"/>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="for UserID" Type="Variable" Display="advanced-hide" Required="true" Mask="false"/>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="for GroupID" Type="Variable" Display="advanced-hide" Required="true" Mask="false"/>
  <Config Name="TZ" Target="TZ" Default="Etc/UTC" Mode="" Description="specify a timezone to use, see this list." Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
</Container>